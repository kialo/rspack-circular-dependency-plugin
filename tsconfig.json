    "compilerOptions": {
        "declaration": true,
        "outDir": "./out",
        "target": "es2015",
        "module": "CommonJS",
        "moduleResolution": "Node",
        "allowJs": true,
        "checkJs": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "lib": ["ES2015", "ES2019.Object"]
    },
    "include": ["index.js"]
}
